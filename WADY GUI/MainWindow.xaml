<Window x:Class="WADY_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WADY_GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="auto" Width="auto">
    <Grid>
        <Menu x:Name="menu" HorizontalAlignment="Left" Height="20" Margin="0,0,-0.4,0" VerticalAlignment="Top" Width="762">
            <MenuItem Header="MenuItem">
                <MenuItem Header="MenuItem" HorizontalAlignment="Left" Width="144"/>
            </MenuItem>
            <MenuItem Header="MenuItem"/>
        </Menu>
        <StatusBar HorizontalAlignment="Left" Height="25" Margin="0,481,-0.4,-0.4" VerticalAlignment="Top" Width="762">
            <StatusBarItem Content="StatusBarItem1" Height="24.8" VerticalAlignment="Top"/>
        </StatusBar>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="461" Margin="0,20,-0.4,0" VerticalAlignment="Top" Width="762">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5" Margin="0,0,0,187.8">
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
